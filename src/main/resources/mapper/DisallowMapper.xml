<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.DisallowMapper">
    
    <sql id="selectFile">
        id,value,type
    </sql>
    <sql id="insertFile">
        value,type
    </sql>

    <select id="selectAll" resultType="DisallowWord">
        select <include refid="selectFile"></include>
        from disallow_word
    </select>
</mapper>